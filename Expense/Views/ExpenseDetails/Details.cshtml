@model ExpenseDetailsViewModel

@{
    ViewData["Title"] = "Expense Details";
}

<h1>@ViewData["Title"]</h1>

<div>
    <hr />
    <dl class="row">
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.Expenseid)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Expenseid)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.ExpenseTypeID)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.ExpenseTypeID)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.Amount)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Amount)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.Remarks)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Remarks)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.NetOfVatAmount)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.NetOfVatAmount)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.VatAmount)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.VatAmount)
        </dd>
    </dl>
</div>
<div>
    <a asp-action="Edit" asp-route-id="@Model?.ExpenseDetailID" class="btn btn-primary">Edit</a>
    <a asp-action="Index" class="btn btn-danger">Close</a>
</div>
